# AI Provider API Configuration
# Anthropic is preferred if both keys are present
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini API Configuration (fallback)
GEMINI_API_KEY=your_gemini_api_key_here

# MongoDB Configuration
# For local MongoDB: mongodb://localhost:27017/scriptforge
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/scriptforge
MONGODB_URI=mongodb://localhost:27017/scriptforge

# Server Configuration
PORT=3000
NODE_ENV=development

# Playwright Configuration
PLAYWRIGHT_BROWSERS_PATH=0
